BibTeX {
  Entries   = Entry+

  Entry     = String
            | Preamble
            | Item

  String    = "@" caseInsensitive<"string"> "{"
              "}"

  Preamble  = "@" caseInsensitive<"preamble"> "{"
              "}"

  Item      = ItemType "{"
              "}"

  ItemType  = "@" letter+
}
